<Window x:Class="DateTimeE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DateTimeE"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="lblInput" Content="Skriv ett datum i formatet yyyy-mm-dd" HorizontalAlignment="Left" Margin="88,81,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtInput" HorizontalAlignment="Left" Margin="103,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnInput" Content="Kör" HorizontalAlignment="Left" Margin="103,135,0,0" VerticalAlignment="Top" Click="btnInput_Click"/>

    </Grid>
</Window>
